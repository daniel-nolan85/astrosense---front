{
  "expo": {
    "name": "NovaQuest",
    "slug": "NovaQuest",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splash.jpg",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "notification": {
      "icon": "./assets/img/icon.png",
      "color": "#009999"
    }
  }
}
